valor= float(input("Digite um valor: "))

if valor>=500.00:
    desconto= valor * 0.20
    produto= valor-desconto
    print(produto)

elif valor<100.00:
    desconto= valor * 0.05
    produto= valor-desconto
    print(produto)

elif valor>=300 and valor<=499:
    desconto= valor * 0.15
    produto= valor-desconto
    print(produto)

if valor>=100 and valor<=299:
    desconto= valor * 0.10
    produto= valor-desconto
    print(produto)
